{
    "name": "@moonsurf/browser-control",
    "version": "1.0.3",
    "description": "AI-native browser automation. MCP server that gives AI agents complete control over web browsers.",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "bin": {
        "moonsurf": "dist/index.js"
    },
    "files": [
        "dist",
        "skills",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "browser-automation",
        "ai",
        "ai-native",
        "llm",
        "chrome",
        "chromium",
        "puppeteer-alternative",
        "selenium-alternative",
        "web-automation",
        "ai-agent",
        "claude",
        "anthropic"
    ],
    "author": "Moonsurf AI",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MoonsurfAI/browser-control.git"
    },
    "homepage": "https://github.com/MoonsurfAI/browser-control#readme",
    "bugs": {
        "url": "https://github.com/MoonsurfAI/browser-control/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.7",
        "@types/node": "^22.10.2",
        "@types/ws": "^8.5.13",
        "adm-zip": "^0.5.16",
        "typescript": "^5.7.2"
    }
}
